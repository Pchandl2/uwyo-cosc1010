import openpyxl
from openpyxl.styles import Color, PatternFill
from pathlib import Path

file_path = Target_Pixel_Art.xlsx
if not Path(file_path).is_file():
    wb = openpyxl.Workbook()
    wb.save(file_path)
else:
    wb = openpyxl.load_workbook(file_path)

ws = wb.active
